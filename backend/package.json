{
  "name": "personal-assistant-backend",
  "version": "1.0.0",
  "description": "Backend API and AI agent for personal assistant",
  "main": "dist/index.js",
  "scripts": {
    "dev": "nodemon --watch src --ext ts --exec tsx src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:integration": "jest --testPathPattern=integration",
    "test:health": "jest --testPathPattern=health",
    "test:services": "jest --testPathPattern=service-integration",
    "test:llm": "jest --testPathPattern=llm-response-quality",
    "test:e2e": "jest --testPathPattern=e2e-workflows",
    "test:api": "jest --testPathPattern=api-integration"
  },
  "keywords": [
    "personal-assistant",
    "ai",
    "mcp"
  ],
  "author": "Justin Pease",
  "license": "MIT",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.32.0",
    "@notionhq/client": "^2.2.15",
    "@supabase/supabase-js": "^2.47.10",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "googleapis": "^149.0.0",
    "node-cron": "^3.0.3",
    "node-fetch": "^3.3.2",
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.12",
    "@types/node": "^22.10.5",
    "@types/node-cron": "^3.0.11",
    "@types/supertest": "^6.0.2",
    "@types/ws": "^8.5.13",
    "jest": "^29.7.0",
    "nodemon": "^3.1.9",
    "supertest": "^7.0.0",
    "ts-jest": "^29.1.2",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3"
  }
}